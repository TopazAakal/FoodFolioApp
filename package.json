  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "react-native-clean-project": "^4.0.3",
    "react-test-renderer": "^18.2.0"
  },
